<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<%= for url <- @urls do %>
  <url>
    <loc><%= url.url %></loc>
    <lastmod><%= DateTime.to_iso8601(url.lastmod) %></lastmod>
    <changefreq><%= url.changefreq %></changefreq>
    <priority><%= url.priority %></priority>
  </url>
<% end %>
</urlset>